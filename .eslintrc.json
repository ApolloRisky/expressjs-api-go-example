{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
        "no-use-before-define": ["error", { "variables": false }],
        "no-multi-str": 0
    }
}